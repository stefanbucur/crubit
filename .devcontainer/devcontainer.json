{
    "name": "Crubit Development",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "mounts": [
        "source=${localEnv:HOME}/.cache/crubit,target=/bazel-cache,type=bind,consistency=cached"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "google.geminicodeassist",
                "BazelBuild.vscode-bazel"
            ]
        }
    }
}